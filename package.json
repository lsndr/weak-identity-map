{
  "name": "weak-identity-map",
  "version": "1.2.1-next.1",
  "description": "The WeakIdentityMap object holds key-value pairs of weakly referenced objects and remembers the original insertion order of the keys",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "lsndr/weak-identity-map",
  "keywords": [
    "weak",
    "map",
    "hash",
    "identity",
    "hashmap",
    "reference"
  ],
  "scripts": {
    "build": "tsc",
    "test": "node --expose-gc node_modules/.bin/jest",
    "test:coverage": "node --expose-gc node_modules/.bin/jest --coverage --no-cache",
    "lint": "eslint \"**/*.{ts,js}\" --max-warnings=0",
    "lint:fix": "npm run lint -- --fix",
    "lint:fix-staged": "lint-staged",
    "prepare": "npm run build"
  },
  "author": {
    "name": "lsndr",
    "email": "alexander@lsndr.me",
    "url": "https://lsndr.me"
  },
  "engines": {
    "node": ">=14.6.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "conventional-changelog-conventionalcommits": "^6.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "lint-staged": "^11.2.3",
    "pre-commit": "^1.2.2",
    "prettier": "^2.4.1",
    "semantic-release": "^21.0.7",
    "ts-jest": "^27.0.6",
    "typescript": "^4.4.4"
  },
  "pre-commit": [
    "test",
    "lint:fix-staged"
  ],
  "lint-staged": {
    "**/*.{ts,js}": "eslint --fix --max-warnings=0"
  }
}
